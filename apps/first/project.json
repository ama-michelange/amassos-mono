{
  "name": "first",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/first/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:application",
      "old-executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/first",
        "browser": "apps/first/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/first/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/first/public"
          },
          {
            "glob": "**/*",
            "input": "dist/apps/favicons/first"
          }
        ],
        "styles": ["apps/first/src/styles.scss"],
        "plugins": [
          "apps/first/src/plug/plugin1.js",
          "apps/first/src/plug/plugin2.js",
          {
            "path": "@anolilab/unplugin-favicons/esbuild",
            "options": {
              "logo": "apps/first/public/img/amassos-logo-2_line-black_green.svg",
              "outputPath": "dist/apps/favicons/first/favicons",
              "inject": false,
              "favicons": {
                "path": "/",
                "appName": "AmAssos Site",
                "pixel_art": true,
                "icons": {
                  "android": true,
                  "appleIcon": false,
                  "appleStartup": false,
                  "favicons": true,
                  "windows": false,
                  "yandex": false
                }
              }
            }
          }
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all",
          "verbose": false
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "verbose": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "first:build:production"
        },
        "development": {
          "buildTarget": "first:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "first:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/first/jest.config.ts",
        "tsConfig": "apps/first/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "first:build",
        "port": 4200,
        "staticFilePath": "dist/apps/first/browser",
        "spa": true
      }
    }
  }
}
